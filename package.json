{
  "name": "Banka",
  "version": "1.0.0",
  "description": "",
  "main": ".server/index.js",
  "scripts": {
    "start": "node -r esm ./server",
    "start:dev": "nodemon --exec node -r esm ./server",
    "dropTable": "node -r esm ./server/PostgreSQL/resetDBTablesOnMigration dropTables",
    "createTable": "node -r esm ./server/PostgreSQL/resetDBTablesOnMigration createTables",
    "migrate-test": "npm run dropTable && npm run createTable",
    "migrate-prod": "npm run createTable",
    "heroku-postbuild": "npm run migrate-prod",
    "test": "NODE_ENV=TEST npm run migrate-test && NODE_ENV=TEST nyc mocha -r esm ./server/__test__/*.js --timeout 10000 --exit",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [],
  "author": "Nurudeen Soremekun",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/polyfill": "^7.4.3",
    "@babel/register": "^7.4.0",
    "babel-preset-airbnb": "^3.2.1",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.0.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "nodemon": "^1.18.11",
    "nyc": "^13.3.0"
  },
  "dependencies": {
    "async": "^3.0.0",
    "bcrypt": "^3.0.5",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^7.0.0",
    "esm": "^3.2.25",
    "express": "^4.16.4",
    "express-session": "^1.16.2",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.6",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "pg": "^7.9.0",
    "pm2": "^5.2.0"
  }
